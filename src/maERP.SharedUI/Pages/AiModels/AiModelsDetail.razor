@page "/AiModels/{aiModelId:int}"
@using Microsoft.AspNetCore.Authorization

@attribute [Authorize]

<MudStack Row="true" Class="mb-8">
    <MudText Typo="Typo.h4">@_title</MudText>
    <MudSpacer />
    <MudIconButton Icon="@Icons.Material.Filled.Edit" Color="Color.Primary" Href="@($"/AiModels/{AiModelId}/edit")" />
    <MudIconButton Icon="@Icons.Material.Filled.ArrowBack" Color="Color.Primary" Href="@($"/AiModels/")" />
</MudStack>

<MudCard>
    <MudCardContent>
        <MudText Typo="Typo.h6">ID: @_aiModelDetail.Id</MudText>
        <MudText Typo="Typo.body1">Name: @_aiModelDetail.Name</MudText>
    </MudCardContent>
</MudCard>